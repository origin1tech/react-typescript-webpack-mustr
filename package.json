{
  "name": "app",
  "version": "1.0.0",
  "description": "React app",
  "keywords": [],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "node ./build clean",
    "copy": "node ./build copy",
    "prod": "NODE_ENV=production BROWSER=true node ./node_modules/webpack/bin/webpack --progress --env=prod --profile --config build/webpack",
    "dev": "NODE_ENV=development BROWSER=true node ./node_modules/webpack-dev-server/bin/webpack-dev-server -d --hot --colors --watch --progress --profile --config build/webpack",
    "build": "npm run clean && npm run copy && npm run prod",
    "commit": "git add . && git commit -am 'auto commit' && git push",
    "bump": "node ./build bump",
    "docs": "node ./node_modules/typedoc/bin/typedoc --out ./docs ./src --options ./typedoc.json",
    "release": "npm run build && npm run docs && npm run bump && npm run commit && npm run push",
    "test": "mocha --opts ./src/mocha.opts"
  },
  "build": {
    "name": "App",
    "browserSync": false,
    "host": "localhost",
    "port": 3000,
    "src": "src",
    "dist": "dist",
    "entry": "main.tsx",
    "assets": "assets",
    "publicPath": "/",
    "template": {
      "title": "App",
      "appMountId": "app"
    },
    "devtool": "source-map",
    "clean": [
      "./dist/**/*.{ts,map,js}"
    ],
    "html": {
      "title": "App",
      "appMountId": "app"
    }
  },
  "dependencies": {
    "configure": "0.0.1",
    "got": "^7.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-promise-middleware": "4.2.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.11",
    "@types/react": "^15.0.20",
    "@types/react-dom": "^0.14.23",
    "@types/react-helmet": "^5.0.0",
    "@types/react-redux": "^4.4.38",
    "@types/react-router": "^4.0.3",
    "@types/react-router-dom": "^4.0.4",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux-logger": "^3.0.0",
    "@types/redux-promise-middleware": "0.0.6",
    "autoprefixer": "^6.7.7",
    "browser-sync-webpack-plugin": "^1.2.0",
    "chai": "^4.0.2",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "inquirer": "^3.1.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.4.2",
    "mustr": "^1.0.4",
    "node-sass": "^4.5.3",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "postcss-url": "^5.1.2",
    "react-addons-test-utils": "^15.4.2",
    "react-helmet": "^5.0.2",
    "react-hot-loader": "^1.3.1",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "stiks": "^1.0.0",
    "style-loader": "^0.16.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "tslint-react": "^2.5.0",
    "typedoc": "^0.7.1",
    "typescript": "^2.2.2",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  },
  "homepage": "https://github.com/origin1tech/app#readme",
  "repository": {
    "type": "git",
